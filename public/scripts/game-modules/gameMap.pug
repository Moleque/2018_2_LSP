section(class='board', id='gameBoard')
    each row in cardsMap
        each val in row
            div(class='card', id='gameCard')
                if val == 0
                    img(class='front-face', src='img/gold.png')
                else if val == 1
                    img(class='front-face', src='img/water.png')
                if val == 2
                    img(class='front-face', src='img/grass.png')
                if val == 3
                    img(class='front-face', src='img/gray.png')
                img(class='back-face', src='img/unopened.png')
